procedimentos subsequenciaII(mem, A[], B[], n, j){
    Se (n < 0 ou j < 0){
        retorna 0
    }
    Se(A[n] == B[j]){
        temp1 = A[n]
    }SenÃ£o{
        temp2 = subsequenciaII(mem, A[], B[], n-1, j-1)
        temp3 = A[n] + subsequenciaII(mem, A[], B[], n-1, j)
        temp4 = B[j] + subsequenciaII(mem, A[], B[], n, j-1)
    }
    retorna max(temp1, temp2, temp3, temp4)


}